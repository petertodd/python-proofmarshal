[
["Test the basic node types and behavior with concatenation to make the structure clear"],

["Empty"],
[{},
 "cat",
 ["00", 0]
],

["Leaf"],
[{"ffffffff":["deadbeef", 1]},
 "cat",
 ["01ffffffffdeadbeef0001", 1]
],

["Inner with left and right leaf children"],
[{"ffffffff":["deadbeef", 1],
  "00000000":["cafebabe", 3]},
 "cat",
 ["02 01ffffffffdeadbeef00010001 0100000000cafebabe00030003", 4]
],

["Inner with collision on first bit"],
[{"ffffffff":["deadbeef", 1],
  "80000000":["cafebabe", 3]},
 "cat",
 ["02  02 01ffffffffdeadbeef00010001 0180000000cafebabe00030003 0004 000000", 4]
],

["First two bits collide"],
[{"ffffffff":["deadbeef", 1],
  "c0000000":["cafebabe", 3]},
 "cat",
 ["02  02 0201ffffffffdeadbeef0001000101c0000000cafebabe00030003 0004 000000 0004  000000", 4]
],

["Three items"],
[{"ffffffff":["deadbeef", 1],
  "bfffffff":["cafebabe", 3],
  "00000000":["baadf00d", 5]},
 "cat",
 ["02  02 01ffffffffdeadbeef00010001 01bfffffffcafebabe00030003 0004 0100000000baadf00d00050005", 9]
],

["Four items"],
[{"ffffffff":["deadbeef", 1],
  "bfffffff":["cafebabe", 3],
  "40000000":["feedface", 5],
  "00000000":["baadf00d", 7]},
 "cat",
 ["02  02 01ffffffffdeadbeef00010001 01bfffffffcafebabe00030003 0004  02 0140000000feedface00050005 0100000000baadf00d00070007 000c", 16]
],


